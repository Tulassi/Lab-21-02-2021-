def main() :
   colors = {"red", "orange", "blue", "green"}  
   if inRainbow(colors) :
      print("rainbow colors")
   else :
      print("not rainbow colors")
def inRainbow(colors) :
   rainbow = {"red", "orange", "yellow", "green", "blue", "indigo", "violet"}
 for color in colors:
      if color in rainbow:
         return True
      else:
         return False
main()
  
   
